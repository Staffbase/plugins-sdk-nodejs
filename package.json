  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "dmd-bitbucket": "^0.1.10",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.10.3",
    "handlebars": "^4.0.6",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "jsonwebtoken": "^7.3.0"
  }
